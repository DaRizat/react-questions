{
  "name": "react-questions",
  "version": "1.4.2",
  "description": "A package for breaking down complex forms into bite-sized questions",
  "main": "bundle.js",
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "styled-components": "^4.2.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "babel-loader": "^8.0.5",
    "rollup": "^1.12.3"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "storybook": "start-storybook",
    "build": "rollup -c rollup.config.js",
    "build:watch": "yarn build --w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaRizat/react-questions.git"
  },
  "keywords": [
    "forms",
    "questions"
  ],
  "author": "Jack Bishop",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DaRizat/react-questions/issues"
  },
  "homepage": "https://github.com/DaRizat/react-questions#readme",
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "jest/globals": true
    },
    "rules": {
      "react/jsx-filename-extension": 0,
      "no-unused-expressions": [
        "error",
        {
          "allowTaggedTemplates": true
        }
      ]
    },
    "plugins": [
      "jest"
    ],
    "globals": {
      "window": true,
      "document": true
    },
    "standard": {
      "parser": "babel-eslint"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    }
  }
}
